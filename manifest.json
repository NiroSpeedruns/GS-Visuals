{
  "manifest_version": 3,
  "name": "HoHs Visuals",
  "description": "Add visual mods for House Of Hazards to be used for content creation or a challenge",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://poki.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
